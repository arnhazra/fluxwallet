{
  "name": "fluxwallet",
  "version": "3.2.0",
  "description": "An Wealth Management Platform",
  "scripts": {
    "dev:core-service": "npm run dev --prefix services/core-service",
    "dev:ui-service": "npm run dev --prefix services/ui-service",
    "init": "npm install && npm install --prefix services/core-service && npm install --prefix services/ui-service",
    "dev": "concurrently \"npm run dev:core-service\" \"npm run dev:ui-service\"",
    "publish": "git add . && git commit -m \"Publish App\" && git push",
    "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\""
  },
  "author": "Arnab Hazra",
  "license": "GNU AFFERO GENERAL PUBLIC LICENSE",
  "devDependencies": {
    "concurrently": "^9.2.1",
    "prettier": "^3.7.4"
  }
}
